            </xs:documentation>
          </xs:annotation>
          <xs:complexType>
            <xs:choice minOccurs="0" maxOccurs="unbounded">
              <xs:element name="ProjectTemplate" minOccurs="0" maxOccurs="1">
                <xs:annotation>
                  <xs:documentation>
